<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="ExamQuizXammarinApp.AdminPanel">
    <!--Pages can be added as references or inline-->

    <StackLayout>

        <Label Text="Add Admin"/>
        <Entry x:Name="txtUsername" Placeholder="Username"/>
        <Entry x:Name="txtPassword" Placeholder="Password"/>
        <Entry x:Name="txtEmail" Placeholder="Email"/>
        <Button Text="Add" Clicked="Button_Clicked_Admin_Add"/>
        <Label Text="Add Question>"/>
        <Entry x:Name="entryQuestion"/>
   
    </StackLayout>

</ContentPage>

    